{
  "name": "sahibinden.com - Fast Classified",
  "version": "1.0",
  "manifest_version": 2,
  "description": "sahibinden quickly create classifieds",
  "incognito": "split",
  "browser_action": {
    "default_icon": "sahibinden.png",
    "default_popup": "index.html"
  },
  "icons": {
    "1": "sahibinden.png"
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "scripts/content.js",
        "scripts/enums.js",
        "scripts/postRulesCheck.js",
        "scripts/address.js",
        "scripts/realestate/realEstate.js",
        "scripts/actionHelper.js",
        "scripts/pocketwatch/pocketWatch.js",
        "scripts/teachingStuff/teachingStuff.js"
      ],
      "all_frames": true,
      "run_at": "document_end"
    }
  ],
  "permissions": [
    "activeTab"
  ],
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'"
}