{  
  "name":"SahiFast Classified",
  "version":"1.0",
  "manifest_version":2,
  "description":"This plugin allows you to quickly create classifieds",
  "incognito": "split",

  "browser_action":
  {  
      "default_icon":"sahibinden.png",
      "default_popup":"index.html"
  }, 

  "content_scripts": 
  [{
    "matches":["<all_urls>"],
    "js": ["scripts/content.js", "scripts/address.js", "scripts/realEstate.js", "scripts/actionHelper.js", "scripts/pocketWatch.js"],
    "all_frames": true,
    "run_at": "document_end"
  }],
   
  "permissions": ["activeTab"],
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'"
}